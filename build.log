+
+  const label = isOverdue
+    ? `Overdue by ${parts.join(" ")}`
+    : `${parts.join(" ")} left`;
+
+  const tone = isOverdue ? "overdue" : deltaMinutes <= 1440 ? "soon" : "normal";
+
+  return { label, tone };
+};
