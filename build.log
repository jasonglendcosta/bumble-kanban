+    return trackedMinutes;
+  }
+
+  const started = new Date(startedAt);
+  if (Number.isNaN(started.getTime())) {
+    return trackedMinutes;
+  }
+
+  const delta = minutesBetween(started, now);
+  return trackedMinutes + Math.max(delta, 0);
+};


